alphabet { "a", "b", "c", "d", "!", "?" }
states { q0, q1, q2, q3 }
terminal { q1, q2 }
state q1 { "есть ?" }
state q2 { "на конце !" }
q0 ---> q1 [ "?" ]
q0 ---> q2 [ "!" ]
q0 ---> q3 [ "a", "b", "c", "d" ]
q1 ---> q1 [ "a", "b", "c", "d", "!", "?" ]
q2 ---> q1 [ "?" ]
q2 ---> q2 [ "!" ]
q2 ---> q3 [ "a", "b", "c", "d" ]
q3 ---> q1 [ "?" ]
q3 ---> q2 [ "!" ]
q3 ---> q3 ["a", "b", "c", "d" ]
